#!/bin/bash
CELL_ID=`basename ${BASH_SOURCE[0]}`
DICT_CELL=("${DICT_CELL[@]}" $CELL_ID)
DICT=(
"食品日用品/35062"
"生鲜餐饮词库/2966"
"蔬菜名/17460"
"各地茶叶饮料名/8813"
"酒类品牌/8541"
"饮料类品牌/8540"

"西餐用语/8121"

"全国餐饮企业名录/8760"

"中国南北大菜-- 菜名大全/9018"
"八大菜系菜单列表/239"
"饮食大全【官方推荐】/15201"
"八大菜系【官方推荐】/76640"

"常用餐饮词汇/11978"
)
eval "${CELL_ID}${DICT_CELL_SUFFIX}=(${DICT[@]})"